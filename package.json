{
  "name": "kalos-ecommerce",
  "version": "1.0.0",
  "description": "Marketplace de servicios de belleza a domicilio en Bolivia",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:unit": "vitest run",
    "test:e2e": "playwright test",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,css,md}",
    "emulators:start": "firebase emulators:start",
    "emulators:export": "firebase emulators:export ./emulator-data",
    "emulators:import": "firebase emulators:start --import ./emulator-data",
    "deploy:staging": "firebase use kalos-dev && firebase deploy --only hosting",
    "deploy:production": "firebase use kalos-prod && firebase deploy --only hosting",
    "analyze": "vite-bundle-analyzer dist"
  },
  "dependencies": {
    "firebase": "^10.12.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.44.0",
    "@vitejs/plugin-legacy": "^5.4.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.3",
    "vite": "^5.2.0",
    "vite-bundle-analyzer": "^0.7.0",
    "vite-plugin-pwa": "^0.20.0",
    "vitest": "^1.6.0"
  },
  "keywords": [
    "beauty",
    "marketplace", 
    "bolivia",
    "services",
    "firebase",
    "vite",
    "tailwind"
  ],
  "author": "Kalos E-commerce Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/barbaritalaram/AIxDev.git"
  },
  "bugs": {
    "url": "https://github.com/barbaritalaram/AIxDev/issues"
  },
  "homepage": "https://kalos.com"
}
